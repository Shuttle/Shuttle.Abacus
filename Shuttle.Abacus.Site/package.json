{
  "name": "abacus",
  "version": "1.0.0",
  "description": "Shuttle.Abacus constraint-based calculation engine.",
  "steal": {
    "main": "main.js",
    "directories": {
      "lib": "src"
    },
    "npmAlgorithm": "flat",
    "plugins": [
      "steal-stache",
      "steal-css"
    ],
    "meta": {
      "popper.js": {
        "format": "global",
        "deps": [
          "jquery"
        ]
      },
      "bootstrap": {
        "deps": [
          "jquery",
          "popper.js"
        ]
      },
      "moment": {
        "format": "global"
      },
      "tempusdominus": {
        "format": "global",
        "deps": [
          "moment"
        ]
      }
    },
    "map": {
      "popper.js": "popper.js/dist/umd/popper",
      "bootstrap": "bootstrap/dist/js/bootstrap",
      "tempusdominus": "tempusdominus-bootstrap-4/build/js/tempusdominus-bootstrap-4.js"
    },
    "serviceBaseURL": "http://localhost:6300/api/",
    "accessBaseURL": "http://localhost:5599/api/",
    "debug": true,
    "localeVersion": "1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "steal-tools bundle --deps --no-minify"
  },
  "author": "Eben Roux",
  "homepage": "https://github.com/Shuttle/Shuttle.Abacus",
  "dependencies": {
    "bootstrap": "^4.1.2",
    "can-component": "^4.3.0",
    "can-compute": "^4.1.0",
    "can-connect": "^3.0.1",
    "can-define": "^2.5.2",
    "can-define-validate-validatejs": "^1.1.0",
    "can-route": "^4.3.0",
    "can-route-pushstate": "^5.0.0",
    "can-set": "^1.6.0",
    "can-stache": "^4.5.10",
    "can-stache-bindings": "^4.3.0",
    "can-stache-route-helpers": "^1.1.0",
    "can-util": "^3.11.5",
    "can-validate-validatejs": "^1.0.0",
    "i18next": "^11.3.6",
    "i18next-xhr-backend": "^1.5.1",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "popper.js": "^1.14.3",
    "shuttle-access": "^1.0.1",
    "shuttle-can-api": "^1.0.18",
    "shuttle-canstrap": "^1.0.59",
    "shuttle-guard": "^1.0.0",
    "steal": "^1.12.3",
    "steal-css": "^1.3.2",
    "steal-stache": "^4.1.2",
    "tempusdominus-bootstrap-4": "^5.0.1",
    "validate.js": "^0.11.1"
  }
}
