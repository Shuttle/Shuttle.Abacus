{{#if map.isResolved}}
    <h5>{{argument.name}} ({{argument.dataTypeName}})</h5>
    <abacus-argument-values-add />
{{else}}
    <cs-fetching title:from="'argument'" />
{{/if}}

{{#if values.isResolved}}
    <cs-table vm:columns:from="columns" vm:rows:from="values" />
{{else}}
    <cs-fetching title:from="'values'" />
{{/if}}
