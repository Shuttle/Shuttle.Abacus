<cs-form>
    <div class="row">
        <div class="col"><cs-form-input focus:from="'true'" label:from="'name'" vm:value:bind="name"/></div>
    </div>
    <cs-button-submit vm:click:from="refresh" text:from="'search'"/>
</cs-form>
{{#if list.isResolved}}
    <cs-table vm:columns:from="columns" vm:rows:from="list.value" />
{{else}}
    <cs-fetching title:from="'arguments'" />
{{/if}}
